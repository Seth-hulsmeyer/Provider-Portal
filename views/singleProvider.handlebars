{{#each data}}
<h1>{{this.dataValues.first_name}} {{this.dataValues.last_name}}</h1>

<div class="card col-sm-4 col-sm-offset-1" style="width: 50rem;">
    <div class="card-body">
        <h5 class="card-title">Practice: {{this.dataValues.practice}}
        </h5>
        <h6 class="card-subtitle mb-2 text-muted">
            Provider ID: {{this.dataValues.id}}</h6>
        <p class="card-text">
            Company: {{this.dataValues.company}} <br />
            Email: {{this.dataValues.email}}<br />
            Phone Number: {{this.dataValues.phone}}<br />
        </p>
    </div>
</div>

{{/each}}
